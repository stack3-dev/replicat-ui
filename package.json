{
  "name": "replicat-ui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "predev": "pnpm run generate",
    "dev": "vite",
    "scan": "vite & npx react-scan@latest localhost:5174",
    "prebuild": "pnpm run generate",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate": "pnpm run generate:wagmi && pnpm run generate:graphql",
    "generate:wagmi": "wagmi generate",
    "generate:graphql": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.14.2",
    "@emotion/react": "^11.14.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@tanstack/react-query": "^5.69.0",
    "@uidotdev/usehooks": "^2.4.1",
    "abitype": "^1.0.8",
    "alchemy-sdk": "^3.5.6",
    "graphql": "^16.10.0",
    "graphql-request": "^7.1.2",
    "next-themes": "^0.4.6",
    "nuqs": "^2.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "urql": "^4.2.2",
    "viem": "~2.23.15",
    "wagmi": "^2.14.15"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^3.14.2",
    "@eslint/js": "^9.21.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@parcel/watcher": "^2.5.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@wagmi/cli": "^2.2.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "es5-ext",
      "esbuild"
    ]
  }
}
